{
  "name": "browser-mask",
  "description": "Provide a mask layer for the browser.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "浏览器遮罩层",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": ["storage", "scripting", "activeTab"]
}